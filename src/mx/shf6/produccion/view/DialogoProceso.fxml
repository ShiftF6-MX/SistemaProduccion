<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefWidth="600.0" styleClass="dashBoardCard" stylesheets="@../application.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mx.shf6.produccion.view.DialogoProceso">
   <children>
      <VBox alignment="CENTER_LEFT" prefHeight="40.0" styleClass="dashBoardCardGray" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label styleClass="textFieldTitulos" text="Formulario proceso">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <VBox.margin>
                  <Insets left="20.0" />
               </VBox.margin></Label>
         </children>
         <opaqueInsets>
            <Insets left="20.0" />
         </opaqueInsets>
      </VBox>
      <VBox maxWidth="250.0" prefWidth="250.0" spacing="10.0" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="30.0" AnchorPane.topAnchor="55.0">
         <children>
            <VBox maxWidth="250.0" spacing="2.0">
               <children>
                  <Label styleClass="labelRobotoMedium12" text="Fecha" />
                  <DatePicker fx:id="campoDateFecha" maxWidth="250.0" styleClass="textFieldTitulosDosBlack" />
               </children>
            </VBox>
            <VBox maxWidth="250.0" spacing="2.0">
               <children>
                  <Label styleClass="labelRobotoMedium12" text="Cantidad" />
                  <TextField fx:id="campoTextoCantidad" maxWidth="250.0" styleClass="campoBuscador" />
               </children>
            </VBox>
            <VBox maxWidth="250.0" spacing="2.0">
               <children>
                  <Label styleClass="labelRobotoMedium12" text="Ordenamiento" />
                  <TextField fx:id="campoTextoOrdenamiento" maxWidth="250.0" styleClass="campoBuscador" />
               </children>
            </VBox>
            <VBox maxWidth="250.0" spacing="2.0">
               <children>
                  <Label styleClass="labelRobotoMedium12" text="Nivel" />
                  <TextField fx:id="campoTextoNivel" maxWidth="250.0" styleClass="campoBuscador" />
               </children>
            </VBox>
            <VBox maxWidth="250.0" spacing="2.0">
               <children>
                  <Label styleClass="labelRobotoMedium12" text="Centro trabajo" />
                  <ComboBox fx:id="comboBoxDestino" maxWidth="250.0" prefWidth="150.0" styleClass="campoBuscador" />
               </children>
            </VBox>
         </children>
      </VBox>
      <VBox maxWidth="250.0" prefWidth="250.0" spacing="10.0" AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="55.0">
         <children>
            <VBox maxWidth="250.0" spacing="2.0">
               <children>
                  <Label styleClass="labelRobotoMedium12" text="Empleados" />
                  <ComboBox fx:id="comboBoxEmpleados" maxWidth="250.0" prefWidth="150.0" styleClass="campoBuscador" />
               </children>
            </VBox>
            <VBox maxWidth="250.0" spacing="2.0">
               <children>
                  <Label styleClass="labelRobotoMedium12" text="Componentes" />
                  <ComboBox fx:id="comboTextoComponentes" maxWidth="250.0" prefWidth="150.0" styleClass="campoBuscador" />
               </children>
            </VBox>
            <VBox spacing="2.0">
               <children>
                  <Label fx:id="tituloDebit" styleClass="labelRobotoMedium12" text="Debit" />
                  <TextField fx:id="campoDebit" styleClass="campoBuscador" />
               </children>
            </VBox>
            <VBox maxWidth="250.0" prefWidth="250.0" spacing="5.0">
               <children>
                  <Button maxWidth="250.0" mnemonicParsing="false" onAction="#manejadorBotonAceptar" prefHeight="30.0" styleClass="buttonGreen" text="Aceptar" />
                  <Button maxWidth="250.0" mnemonicParsing="false" onAction="#manejadorBotonCerrar" onMouseClicked="#manejadorBotonCerrar" prefHeight="30.0" styleClass="buttonRed" text="Cancelar">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                     <VBox.margin>
                        <Insets top="2.0" />
                     </VBox.margin>
                  </Button>
               </children>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
